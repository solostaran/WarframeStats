extends layout

block content
	script(nonce=nonce).
		document.getElementById('nav-rivtyp').classList.add('active');

	div(class='container-lg')
		div(class='row')
			table(class='table table-responsive table-striped table-hover')
				thead
					tr
						th.
							List of #[a(href='https://warframe.fandom.com/wiki/Riven_Mods') Riven] Types
				tbody
					for type in types
						tr
							td= type.name
